{
  "name": "react-noti",
  "version": "0.0.0-semantically-released",
  "description": "react-noti - Toast Notifications made easy",
  "main": "dist/react-noti.js",
  "module": "dist/react-noti.js",
  "style": "dist/react-noti.css",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --config webpack.config.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "coverage": "npm test -- --coverage",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "lint:prettier": "prettier \"**/*.{md,json,yml,css}\" --check",
    "lint:prettier:fix": "prettier \"**/*.{md,json,yml,css}\" --write",
    "lint:stylelint": "stylelint --ignore-path .gitignore --syntax scss **/*.scss",
    "lint:stylelint:fix": "stylelint --ignore-path .gitignore --syntax scss **/*.scss --fix",
    "build": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false webpack -p --progress --config webpack.config.umd.js",
    "build:examples": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false webpack -p --progress --config webpack.config.js",
    "analyze": "npm run build -- --env.analyze",
    "analyze:lib": "npm run build:lib -- --env.analyze",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vitaliiburlaka/react-noti.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Vitalii Burlaka <vitalii.burlaka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vitaliiburlaka/react-noti/issues"
  },
  "homepage": "https://github.com/vitaliiburlaka/react-noti",
  "keywords": [
    "react",
    "notification",
    "toast",
    "react-noti",
    "push",
    "alert"
  ],
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@hot-loader/react-dom": "^16.13.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.4",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cz-conventional-changelog": "^3.1.0",
    "doctoc": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^4.0.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "semantic-release": "^17.0.7",
    "style-loader": "^1.2.1",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^3.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ignore-path .gitignore --fix"
    ],
    "*.{md,json,yml,yaml,css}": [
      "prettier --write"
    ],
    "*.scss": [
      "stylelint --syntax scss --fix"
    ],
    "README.md": [
      "doctoc --maxlevel 4 --notitle"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run coverage"
    }
  },
  "browserslist": [
    "defaults",
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
